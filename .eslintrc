{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:flowtype/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "flowtype",
    "compat"
  ],
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": true
    }
  },
  "rules": {
    "no-warning-comments": 0,
    "no-console": ["error", { "allow": ["log", "error"] }],
    "no-undef": 0,
    "flowtype/no-types-missing-file-annotation": 0
  },
  "parserOptions": {
    "sourceType": "module",
  }
}
